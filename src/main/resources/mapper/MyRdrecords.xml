<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
    <mapper namespace="com.sun.bosen.mapper.MyRdrecordsMapper"> 
    	<select id="addRerdcords" parameterType="Rdrecords">
    	        
    		insert into My_RdRecords (ID,cInvCode,iQuantity,
			iUnitCost,iPrice,iAPrice,cDefine25,iPOsID,fACost,iTaxCost,iNQuantity,iTaxPrice,iTaxRate,iSum,iNum,cDefine22)
			values (#{iD},#{cInvCode},#{iQuantity},#{iUnitCost},#{iPrice},#{iAPrice},#{cDefine25},
			#{iPOsID},#{fACost},#{iTaxCost},#{iNQuantity},#{iTaxPrice},#{iTaxRate},#{iSum},0,#{cDefine22})
    		
    	</select>
    </mapper>